{
  "name": "zdzz",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "reload": "pnpm run build && pnpm run dev",
    "dev": "pnpm --filter ./examples run dev",
    "build:examples": "pnpm --filter ./examples run build",
    "build": "pnpm --filter=@zdzz/* run build",
    "build:shared": "pnpm --filter=@zdzz/shared run build",
    "build:components": "pnpm --filter=@zdzz/components run build",
    "build:hooks": "pnpm --filter=@zdzz/hooks run build",
    "version:patch": "pnpm --filter=@zdzz/* run version:patch",
    "version:patch:shared": "pnpm --filter=@zdzz/shared run version:patch",
    "version:patch:hooks": "pnpm --filter=@zdzz/hooks run version:patch",
    "clear": "pnpm run clear:packages && pnpm run clear:examples",
    "clear:packages": "pnpm --filter=@zdzz/* run clear",
    "clear:examples": "pnpm --filter ./examples run clear",
    "changeset": "changeset",
    "changeset:publish": "pnpm changeset publish",
    "version-packages": "changeset version"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/jquery": "^3.5.16",
    "@types/lodash-es": "^4.17.6",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.35.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "stylelint": "^15.2.0",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-order": "^6.0.2",
    "typescript": "^4.9.5",
    "unplugin-vue-define-options": "^1.2.3",
    "vite": "^4.1.4",
    "vite-plugin-build": "^0.7.1",
    "vite-plugin-dts": "^2.0.2",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@zdzz/components": "workspace:*",
    "@zdzz/hooks": "workspace:*",
    "@zdzz/shared": "workspace:*",
    "axios": "^1.3.4",
    "jquery": "^3.6.3",
    "lodash-es": "^4.17.21",
    "pdfjs-dist": "^3.4.120",
    "qs": "^6.11.0",
    "sass": "^1.58.3",
    "vue": "^3.2.20",
    "vue-router": "^4.1.6"
  }
}
