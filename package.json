  "scripts": {
    "clear": "rm -rf ./node_modules ./**/**/node_modules ./**/**/dist ./**/**/es ./**/**/lib",
    "dev": "pnpm --filter ./examples run dev",
    "build": "pnpm --filter=@zdzz/* run build",
    "build:shared": "pnpm --filter=@zdzz/shared run build",
    "build:hooks": "pnpm --filter=@zdzz/hooks run build",
    "version:patch": "pnpm --filter=@zdzz/* run version:patch",
    "version:patch:shared": "pnpm changeset version --include=@zdzz/shared",
    "version:patch:hooks": "pnpm changeset version --include=@zdzz/hooks --tag patch",
    "clear:packages": "pnpm --filter=@zdzz/* run clear",
    "clear:examples": "pnpm --filter ./examples run clear",
    "changeset": "changeset",
    "changeset:publish": "pnpm changeset publish",
    "version-packages": "changeset version"
  },
